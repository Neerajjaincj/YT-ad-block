{
    "name": "YouTube Ad Skipper",
    "version": "1.0",
    "description": "Automatically skips YouTube ads.",
    "manifest_version": 3,
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png"
    },
    "permissions": [
      "tabs",
      "activeTab"
    ],
    "content_scripts": [{
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"]
      }]
  }